{
    "manifest_version": 3,
    "name": "Save URL Extension",
    "version": "1.0",
    "permissions": ["activeTab", "storage", "tabs", "http://localhost:3000/"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.jpg",
        "48": "images/icon48.jpg",
        "128": "images/icon128.jpg"
      }
    }
  }
  